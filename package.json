    "json5": "^2.1.0",
    "node-fetch": "^2.6.0",
    "rimraf": "^2.6.3"
  },
  "bugs": {
    "url": "https://github.com/poifuture/golink-gh-pages/issues"
  },
  "homepage": "https://github.com/poifuture/golink-gh-pages#readme",
